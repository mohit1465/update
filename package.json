{
  "name": "electron-auto-update-app",
  "version": "1.3.2",
  "description": "Electron app with auto-update functionality",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "publish": "electron-builder -p always"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohit1465/update.git"
  },
  "build": {
    "appId": "com.electron.autoupdateapp",
    "productName": "AutoUpdateApp",
    "win": {
      "target": ["nsis"],
      "publish": ["github"]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "github",
      "owner": "mohit1465",
      "repo": "update"
    }
  },
  "devDependencies": {
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "fs-extra": "^8.1.0"
  }
}
